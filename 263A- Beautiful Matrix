#include <iostream>
#include<cstdlib>
using namespace std;
 
int main()
{
    int mat[5][5];
    int a=0,b=0,move=0;
    for(int i=0;i<5;i++)
    {
        for(int j=0;j<5;j++)
        {
            cin>>mat[i][j];
           
        }
    }
    for(int i=0;i<5;i++)
    {
        for(int j=0;j<5;j++)
        {
            if(mat[i][j]==1)
            {
                a=i;
                b=j;
            }
           
        }
    }
    move=abs(a-2)+abs(b-2);
    cout<<move;
    
    
}
